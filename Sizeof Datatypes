// Size of Datatypes
#include<iostream>
int main()
{
        using namespace std;
        cout<<"The size of int :"<<sizeof(int)<<endl;
        cout<<"The size of char :"<<sizeof(char)<<endl;
        cout<<"The size of float :"<<sizeof(float)<<endl;
        cout<<"The size of double :"<<sizeof(double)<<endl;
        return 0;
} 
